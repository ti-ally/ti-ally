    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "bootstrap-icons": "^1.10.3",
    "classnames": "^2.3.2",
    "debug": "^4.3.4",
    "next": "13.1.6",
    "next-image-export-optimizer": "^1.3.3",
    "postcss": "^8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-window": "^1.8.8",
