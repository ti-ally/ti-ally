    "clean:export": "rm -rf _out",
    "serve": "serve _out",
    "deploy": "gh-pages --dotfiles --dist _out/ti-ally",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "private": true,
  "dependencies": {
